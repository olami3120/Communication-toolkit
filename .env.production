# API Keys (Replace with your production keys)
VITE_WEATHER_API_KEY=your_production_weather_api_key
VITE_GOOGLE_CALENDAR_API_KEY=your_production_google_calendar_api_key
VITE_TIMEZONE_API_KEY=your_production_timezone_api_key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# Performance Settings
VITE_CACHE_DURATION=3600
VITE_API_TIMEOUT=30000

# Security Settings
VITE_ENABLE_2FA=true
VITE_SESSION_TIMEOUT=3600 